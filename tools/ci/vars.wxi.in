<!-- File to define build variables for the RDMnet installer project. -->
<Include>
  <?define VersionNumber = "@RDMNET_VERSION_STRING_FULL@" ?>

  <?if $(var.Platform) = x64 ?>
    <?define ProgramFilesFolder = "ProgramFiles64Folder" ?>
    <?define PkgDescription = "ETC RDMnet 64-bit Installer" ?>
    <?define VcRedistName = "vcredist_x64.exe" ?>
    <?ifdef RDMnetInstall_x64.TargetPath ?>
      <?define RdmnetTargetPath = "$(var.RDMnetInstall_x64.TargetPath)" ?>
    <?endif ?>
  <?else ?>
    <?define ProgramFilesFolder = "ProgramFilesFolder" ?>
    <?define PkgDescription = "ETC RDMnet 32-bit Installer" ?>
    <?define VcRedistName = "vcredist_x86.exe" ?>
    <?ifdef RDMnetInstall_x86.TargetPath ?>
      <?define RdmnetTargetPath = "$(var.RDMnetInstall_x86.TargetPath)" ?>
    <?endif ?>
  <?endif ?>

</Include>